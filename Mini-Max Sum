nums <- scan("stdin", sep=" ") 
len <- length(nums)

temp <- 0

for(i in 1 : (len - 1)){
  
  for (j in 1 : (len - 1)){
    
    if(nums[j] > nums[j+1]){
      temp <- nums[j]
      nums[j] <- nums[j+1]
      nums[j+1] <- temp
    }
  }
}
min_val <- sum(nums[1:len-1])
max_val <- sum(nums[2:len])
ans <- c(min_val, max_val)
cat(ans)
#write.table(ans, stdout(),row.names = FALSE, col.names = FALSE, quote = FALSE, sep = " ")
